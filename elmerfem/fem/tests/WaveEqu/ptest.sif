Check Keywords "Warn"

$ pival = 2.0 * acos(0.0)

Header
  Mesh DB "." "square"
End

Simulation
  Coordinate System = "Cartesian 2D"
  Simulation Type = Transient
  Timestep Sizes = 0.05
  Timestep Intervals = 50
  Output Intervals(1) = 1
  Steady State Max Iterations = 1
  Max Output Level = 5
!  Post File = "wave.vtu"
End

Body 1
  Equation = 1
  Initial Condition = 1
  Material = 1
!  Body Force = 1
End

Initial Condition 1
  P = Real 0
End

Equation 1
  Active Solvers(1) = 1
End

Material 1
  Sound Speed = Real 0.5
  Sound Damping = Real 0.0
  Sound Reaction Damping = Real 0.0
End 

!Body Force 1
!  Sound Source = Real 0.0
!End

Solver 1
  Equation = "Wave Equation"
  Variable = "P"
  Variable DOFs = 1
  Procedure = "WaveSolver" "WaveSolver"

  Linear System Solver = "Direct"
  Linear System Direct Method = Umfpack
  Steady State Convergence Tolerance = 1e-09
End

Boundary Condition 1
  Target Boundaries(1) = 4

  P = Variable Time
    Real MATC "sin(2.0 * pival * tx)" 

!  Source Acceleration = Variable Time
!    Real MATC "-cos(2.0 * pival * tx) * 4.0 * pival"
End

Boundary Condition 2
  Target Boundaries(1) = 2
  Outflow Boundary = True
End

Solver 1 :: Reference Norm = 6.92823977E-01
