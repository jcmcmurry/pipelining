!---------------------------------------------------------------------
! Solves an elasticity equation with given force when the displacement is 
! known to be fixed. 
!
! P.R. / 4.12.2014
!--------------------------------------------------------------------

Header
  CHECK KEYWORDS Warn
  Mesh DB "." "block"
  Include Path ""
  Results Directory ""
End

Simulation
  Max Output Level = 5
  Coordinate System = "Cartesian"
  Coordinate Mapping(3) = 1 2 3

  Simulation Type = "Steady State"
  Steady State Max Iterations = 1
  Output Intervals = 1

  Post File = "case.vtu"
End

Constants
  Gravity(4) = 0 -1 0 9.82
  Stefan Boltzmann = 5.67e-08
End

Body 1
  Name = "Body1"
  Body Force = 1
  Equation = 1
  Material = 1
End

Equation 1
  Name = "Equation1"
  Active Solvers(1) = 1
End



Solver 1
  Equation = "ElasticSolve"
  Variable = -dofs 2 Displacement
  Procedure = File "ElasticSolve" "ElasticSolver"

  Linear System Solver = "Iterative"
  Linear System Iterative Method = "bicgstabl"
  Linear System Max Iterations = 500
  Linear System Convergence Tolerance = 1.0e-12
  Linear System Preconditioning = ILU1
  Linear System Residual Output = 20

  NonLinear System Max Iterations = 30
  NonLinear System Convergence Tolerance = 1.0e-6

  Calculate Boundary Fluxes = Logical True
!  Calculate Stresses = True
End  


Solver 2
  Exec Solver = never

  Procedure = "SaveData" "SaveScalars"
  Filename = f.dat
  Variable 1 = Displacement 2
  Operator 1 = boundary min
  Operator 2 = boundary max
End 


Material 1
  Name = "Material1"

  Density = 1.0
  Poisson Ratio = 0.3
  Youngs Modulus = 1.0e3
End

Body Force 1
  Name = "BodyForce1"
  Heat Source = 1.0
End

Boundary Condition 1
  Target Boundaries(1) = 1

  Displacement 1 = 0.0
  Displacement 2 = 0.0
End

Boundary Condition 2
  Target Boundaries(1) = 2 

  Force 2 = -10.0
  Force 2 Normalize By Area = Logical True
  Displacement 2 Constant = Logical True

  Save Scalars = True
End

Solver 1 :: Reference Norm = Real $ 7.74550915E-03

