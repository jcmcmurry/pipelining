! Test case for internal partitioning
! Step 1) : partitioning 

Header
  CHECK KEYWORDS Warn
  Mesh DB "." "angle"
  Include Path ""
  Results Directory ""
End

Simulation
  Max Output Level = 5

  Coordinate System = "Cartesian"
  Coordinate Mapping(3) = 1 2 3

  Simulation Type = "Steady State"
  Steady State Max Iterations = 1
  Output Intervals = 1

!  Max Output Partition = Integer 10
End

Constants
  Gravity(4) = 0 -1 0 9.82
  Stefan Boltzmann = 5.67e-08
End

Body 1
  Name = "Body"
  Body Force = 1
  Equation = 1
  Material = 1
End

Equation 1
  Name = "Equations"
  Active Solvers(1) = 1
End

Solver 1
  Exec Solver = "Always"
  Procedure = "PartitionMesh" "PartitionMeshSolver"
  Equation = "Partition"
  Variable = "Partition"

!  No Matrix = Logical True
!  Optimize Bandwidth = False

  Partitioning Method = String "Directional"
!  Partitioning Method = String "Uniform"

!  Number Of Partitions = Integer 3
  Partitioning Divisions(3) = Integer 2 3 1

  Output Directory = File angle
End

Solver 2
  Exec Solver = never  

  Equation = "ResultOutput"
  Procedure = "ResultOutputSolve" "ResultOutputSolver"
  Output File Name = "part" 

!  Vtu format = Logical True
!  Discontinuous Galerkin = True
End


Material 1
  Name = "Material"
  Density = 1
  Heat Conductivity = 1
End

Body Force 1
  Name = "BodyForce"
  Heat Source = 1
End

Boundary Condition 1
  Name = "Constraint"
  Target Boundaries(1) = 1 
  Temperature = 0
End

Boundary Condition 2
  Name = "Constraint"
  Target Boundaries(1) = 2 
  Temperature = 0
End

