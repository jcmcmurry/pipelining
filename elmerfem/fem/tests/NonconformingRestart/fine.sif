Header
  CHECK KEYWORDS Warn
  Mesh DB "." "angle_fine"
  Include Path ""
  Results Directory ""
End

Simulation
  Max Output Level = 20

  Coordinate System = "Cartesian 2D"
  Coordinate Mapping(3) = 1 2 3

  Simulation Type = "Steady State"
  Steady State Max Iterations = 1
  Output Intervals = 1

  Post File = "fine.vtu"

  Initialize Dirichlet Conditions = False
  Restart Before Initial Conditions = Logical True
  Restart Error Continue = Logical True
  Use Mesh Projector = Logical False
End

Constants
  Gravity(4) = 0 -1 0 9.82
  Stefan Boltzmann = 5.67e-08
End

Body 1
  Name = "Body"
  Body Force = 1
  Equation = 1
  Material = 1
  Initial Condition = 1 
End

Initial Condition 1
  Name = "Interp"
  TempB = Equals "TempA"
End 


Equation 1
  Name = "Equations"
  Active Solvers(2) = 1 2
End

Solver 1
  Equation = "Heat Equation"

  Procedure = "HeatSolve" "HeatSolver"
  Variable = "TempB"

  Linear System Solver = "Iterative"
  Linear System Iterative Method = "bicgstab"
  Linear System Max Iterations = 1
  Linear System Convergence Tolerance = 1.0e-08
  Linear System Abort Not Converged = False
  Linear System Preconditioning = "none"
  Linear System Residual Output = 1
  Steady State Convergence Tolerance = 1.0e-05
  Nonlinear System Convergence Tolerance = 1.0e-05
  Nonlinear System Max Iterations = 1
  Nonlinear System Newton After Iterations = 3
  Nonlinear System Newton After Tolerance = 1.0e-02
  Nonlinear System Relaxation Factor = 1.0
End

Solver 2
  Equation = "CoarseRestart"
  Procedure = "AllocateSolver" "AllocateSolver"

  Variable = TempA

! The library functionality does everything, the solver is just needed
! because it provides placeholder for the structures.
  Exec Solver = never
  Mesh = "angle_coarse"
  Restart File = File "coarse.dat"
  Restart Error Continue = Logical True
End 

Material 1
  Name = "Material"
  Density = 1
  Heat Conductivity = 1
End

Body Force 1
  Name = "BodyForce"
  Heat Source = 1
End

Boundary Condition 1
  Name = "Constraint"
  Target Boundaries(1) = 1 
  TempB = Real 0
End

Solver 1 :: Reference Norm = Real 8.03173703E-02
Solver 1 :: Reference Norm Tolerance = Real 0.01

