! Solve the Reissner-Naghdi shell equations based on elementwise lines of curvature parameterization

$ t = 1.0e-2

Check Keywords "Warn"

Header
  Mesh DB "." "plates"
End

Simulation
  Max Output Level = 4
  Coordinate System = Cartesian 3D  !** This affects what ElementInfo returns (3D for getting right metric)
  Simulation Type = Steady
  Output Intervals(1) = 1
  Steady State Max Iterations = 1
!  Post File = "shellplate.vtu"
End

Body 1
  Equation = 1
  Material = 1
  Body Force = 1
End


Material 1
 Poisson Ratio = Real 0.3
 Youngs Modulus = Real 7.0e+10
 Shell Thickness = Real $t
End

Body Force 1 
  Normal Pressure = 1e6
End

Equation 1 :: Active Solvers(1) = 1

Solver 1
  Equation = "Shell equations"
  Procedure = "ShellSolver" "ShellSolver"

  Large Deflection = False

  Linear System Solver = "Iterative"
  Linear System Preconditioning = ILU0
!  Linear System Row Equilibration = Logical True
  Linear System Max Iterations = 1000
  Linear System Convergence Tolerance = 1e-8
  Linear System Iterative Method = GCR
  Linear System GCR Restart = 300
  Linear System Abort Not Converged = False
  Steady State Convergence Tolerance = 1e-09
End

! ----------------------------------------------------
! BCs are defined with respect the global XYZ system:
! ----------------------------------------------------

Boundary Condition 1
  Target Boundaries(1) = 1
  U 1 = Real 0
  U 2 = Real 0
  U 3 = Real 0
  DNU 1 = Real 0
  DNU 2 = Real 0
!  DNU 3 = Real 0     ! No BC => let the energy principle adjust the normal strain
End

Solver 1 :: Reference Norm = Real 0.34686716
Solver 1 :: Reference Norm Tolerance = Real 1.0e-5
